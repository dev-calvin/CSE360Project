//populates list on the main window
public class ListItem {
	int priorityNumber;
	int dueMonth;
	int dueDay;
	String title;
	
	Status (another object?)
	Status (string or enum)
	Date month (int)
	Date day (int)
	
	
	FUNCTIONS
	
	ListItem
	
	public void setPriorityNumber(int newNum){
		priorityNumber = newNum;
	}
	
	public void setTitle(String newTitle){
		title = newTitle;
	}
	
	Set due date (month, day)
	Change status (month, day)
}
